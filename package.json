{
  "name": "@kentico/kontent-angular-http-service",
  "version": "4.4.0-0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Kentico/kontent-angular-http-service"
  },
  "author": "richard sustek",
  "license": "MIT",
  "description": "Implementation of IHttpService using Angular HttpClient",
  "keywords": [
    "Kontent.ai Angular HTTP Client",
    "Kontent.ai Angular ",
    "Kontent.ai HTTP Client"
  ],
  "private": false,
  "scripts": {
    "release": "standard-version",
    "release:beta": "standard-version --prerelease",
    "build:lib": "ng build kontent-angular-http-service",
    "publish:lib": "npm run build:lib && npm publish dist --access public",
    "publish:lib:beta": "npm run build:lib && npm publish dist --access public --tag=next",
    "prepublishOnly": "npm run ts-lint:check && node prevent-direct-publish",
    "build:all": "npm run build:lib",
    "test:all": "npm run build:lib",
    "ts-lint-local": "./node_modules/.bin/tslint",
    "ts-lint:fix": "npm run ts-lint:check -- --fix",
    "ts-lint:check": "npm run ts-lint-local -- --project ./tsconfig.json"
  },
  "peerDependencies": {
    "@angular/common": "^14.0.0",
    "@angular/core": "^14.0.0",
    "@kontent-ai/core-sdk": "^10.0.0",
    "rxjs": "^7.5.0"
  },
  "devDependencies": {
    "@kontent-ai/core-sdk": "^10.0.0",
    "standard-version": "9.5.0",
    "@angular/animations": "14.1.2",
    "@angular/common": "14.1.2",
    "@angular/compiler": "14.1.2",
    "@angular/core": "14.1.2",
    "@angular/http": "7.2.16",
    "@angular/platform-browser": "14.1.2",
    "@angular/platform-browser-dynamic": "14.1.2",
    "core-js": "3.24.1",
    "rxjs": "7.5.6",
    "zone.js": "0.11.8",
    "@kentico/kontent-core": "9.5.0",
    "@angular/compiler-cli": "14.1.2",
    "@angular-devkit/build-ng-packagr": "0.1002.0",
    "@angular-devkit/build-angular": "14.1.2",
    "ng-packagr": "14.1.0",
    "tslib": "2.4.0",
    "typescript": "4.7.4",
    "@angular/cli": "14.1.2",
    "@types/node": "18.7.6",
    "tslint": "6.1.3"
  }
}
