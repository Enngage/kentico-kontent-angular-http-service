{
  "name": "@kentico/kontent-angular-http-service",
  "version": "4.2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Kentico/kontent-angular-http-service"
  },
  "author": "richard sustek",
  "license": "MIT",
  "description": "Implementation of IHttpService using Angular HttpClient",
  "keywords": [
    "Kentico Kontent",
    "Kentico Kontent Angular",
    "Kentico Kontent Http service"
  ],
  "private": false,
  "scripts": {
    "release": "standard-version",
    "prerelease": "standard-version --prerelease",
    "build:lib": "ng build kontent-angular-http-service",
    "publish:lib": "npm run build:lib && npm publish dist --access public",
    "prepublishOnly": "npm run ts-lint:check && node prevent-direct-publish",
    "build:all": "npm run build:lib",
    "test:all": "node fake-test && npm run build:lib",
    "test:travis": "node fake-test",
    "ts-lint-local": "./node_modules/.bin/tslint",
    "ts-lint:fix": "npm run ts-lint:check -- --fix",
    "ts-lint:check": "npm run ts-lint-local -- --project ./tsconfig.json"
  },
  "peerDependencies": {
    "@angular/common": "^12.0.0",
    "@angular/core": "^12.0.0",
    "@kentico/kontent-core": "^9.2.0",
    "rxjs": "^6.0.0"
  },
  "devDependencies": {
    "standard-version": "9.3.1",
    "@angular/animations": "12.2.8",
    "@angular/common": "12.2.8",
    "@angular/compiler": "12.2.8",
    "@angular/core": "12.2.8",
    "@angular/http": "7.2.16",
    "@angular/platform-browser": "12.2.8",
    "@angular/platform-browser-dynamic": "12.2.8",
    "core-js": "3.18.2",
    "rxjs": "6.6.3",
    "zone.js": "0.11.4",
    "@kentico/kontent-core": "9.2.0",
    "@angular/compiler-cli": "12.2.8",
    "@angular-devkit/build-ng-packagr": "0.1002.0",
    "@angular-devkit/build-angular": "12.2.8",
    "ng-packagr": "12.2.2",
    "tslib": "2.3.1",
    "typescript": "4.2.4",
    "@angular/cli": "12.2.8",
    "@types/node": "16.10.3",
    "tslint": "6.1.3"
  }
}
